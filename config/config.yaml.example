# ====== VEZHA API ======
vezha:
  api:
    base-url: ""
    token: ""

  # ====== Cafeteria ======
  cafe:
    # absolute folder for XLSX output
    output-dir: "C:/reports/cafeteria"
    # analytics ID(s) used to count unique people (list)
    analytics-ids: [2]
    # local scheduling/meal-time timezone
    timezone: "Asia/Yerevan"
    # daily run time (22:00 local time)
    schedule-cron: "0 0 22 * * *"
    # exclude these lists completely (case-insensitive names) (list)
    excluded-list-names: ["outsourced"]

# ====== Telegram ======
telegram:
  bot:
    token: ""
    username: ""

# ====== Evacuation ======
evacuation:
  # сколько дней истории смотреть при поиске последнего входа/выхода; 0 — всё время
  lookbackDays: 14
  # период обновления статусов в минутах
  refreshMinutes: 5
  # идентификаторы списков, для которых вычислять статусы
  listIds: [2]

# ====== PostgreSQL ======
postgres:
  # путь до psql.exe
  psqlPath: "C:/ProgramData/Incoresoft/PostgreSQL/bin/psql.exe"
  # суперпользователь и пароль (для создания БД/таблицы)
  superuser: "postgres"
  superpass: "postgres"
  # название БД, куда будет создана таблица evacuation
  database: "dilijancustomization"
